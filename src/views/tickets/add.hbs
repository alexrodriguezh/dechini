<div class="page">
    <div class="page-content d-flex align-items-stretch">
        {{> navigation }}
        <div class="content-inner">
            <!-- Page Header-->
            <header class="page-header">
                <div class="container-fluid">
                    <h2 class="no-margin-bottom">Dashboard</h2>
                </div>
            </header>
            <div class="page-content d-flex align-items-stretch">
                {{!-- Editar --}}
                <div class="container p-4">
                    <div class="row">
                        <div class="col-md-4 mx-auto">
                            <div class="card">
                                <div class="card-body">
                                    <form action="/tickets/add" method="POST">
                                        <div class="form-group">
                                            <input type="text" name="title" placeholder="Title" autofocus id="" class='form-control'>
                                        </div>
                                        <div class="form-group">
                                            <input type="url" name="url" class="form-control" placeholder="URL" id="">
                                        </div>
                                        <div class="form-group">
                                            <textarea name="description" rows="2" class="form-control" placeholder="Description"></textarea>
                                        </div>
                                        <div class="form-group">
                                            <input type="text" name="categoria" class="form-control" placeholder="Categoria" id="">
                                        </div>
                                        <div class="form-group">
                                            <button class="btn btn-success btn-block">
                                                Guardar
                                            </button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                {{!-- Fin Editar --}}
            </div>
            {{> footer}}
        </div>
    </div>
</div>